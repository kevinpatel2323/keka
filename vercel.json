{
  "crons": [
    {
      "path": "/api/cron?action=in",
      "schedule": "30 3 * * 1-5"
    },
    {
      "path": "/api/cron?action=out",
      "schedule": "00 13 * * 1-5"
    }
  ],
  "builds": [
    {
      "src": "api/cron.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/cron",
      "dest": "api/cron.py"
    }
  ]
}
